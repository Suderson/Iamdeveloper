<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_intern_room"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.suderson.iamdeveloper.InternRoom"
    android:background="@drawable/bg3">
    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:orientation="vertical" android:layout_width="match_parent"
        android:layout_height="match_parent">

    </LinearLayout>

    <RelativeLayout
        android:layout_width="372dp"
        android:layout_height="497dp"
        android:layout_centerVertical="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true">

        <Button
            android:text="seat 1"
            android:layout_width="60dp"
            android:layout_height="70dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="10dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="17dp"
            android:id="@+id/button5"
            style="@style/Widget.AppCompat.Button.Borderless.Colored"/>

        <Button
            android:text="seat 3"
            android:layout_width="60dp"
            android:layout_height="70dp"
            android:layout_below="@+id/button6"
            android:layout_alignRight="@+id/button6"
            android:layout_alignEnd="@+id/button6"
            android:layout_marginTop="50dp"
            android:id="@+id/button16"
            style="@style/Widget.AppCompat.Button.Borderless.Colored" />

        <Button
            android:text="seat 4"
            android:layout_width="60dp"
            android:layout_height="70dp"
            android:layout_marginTop="50dp"
            android:id="@+id/button17"
            android:layout_below="@+id/button16"
            android:layout_alignLeft="@+id/button16"
            android:layout_alignStart="@+id/button16"
            style="@style/Widget.AppCompat.Button.Borderless.Colored" />

        <Button
            android:text="seat5"
            android:layout_width="60dp"
            android:layout_height="70dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_marginRight="14dp"
            android:layout_marginEnd="14dp"
            android:id="@+id/button19"
            style="@style/Widget.AppCompat.Button.Borderless.Colored" />

        <Button
            android:text="seat 6"
            android:layout_width="60dp"
            android:layout_height="70dp"
            android:layout_above="@+id/button16"
            android:layout_alignLeft="@+id/button19"
            android:layout_alignStart="@+id/button19"
            android:layout_marginBottom="23dp"
            android:id="@+id/button20"
            style="@style/Widget.AppCompat.Button.Borderless.Colored" />

        <Button
            android:text="seat 7"
            android:layout_width="60dp"
            android:layout_height="70dp"
            android:layout_above="@+id/button17"
            android:layout_alignLeft="@+id/button20"
            android:layout_alignStart="@+id/button20"
            android:layout_marginBottom="21dp"
            android:id="@+id/button21"
            style="@style/Widget.AppCompat.Button.Borderless.Colored" />

        <Button
            android:text="seat 8"
            android:layout_width="60dp"
            android:layout_height="70dp"
            android:layout_alignBottom="@+id/button17"
            android:layout_alignRight="@+id/button21"
            android:layout_alignEnd="@+id/button21"
            android:layout_marginBottom="20dp"
            android:id="@+id/button22"
            style="@style/Widget.AppCompat.Button.Borderless.Colored" />

        <Button
            android:text="seat 9"
            android:layout_width="60dp"
            android:layout_height="70dp"
            android:layout_below="@+id/button22"
            android:layout_alignLeft="@+id/button22"
            android:layout_alignStart="@+id/button22"
            android:layout_marginTop="26dp"
            android:id="@+id/button23"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            style="@style/Widget.AppCompat.Button.Borderless.Colored" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/ic_setmore"
            android:layout_centerVertical="true"
            android:layout_centerHorizontal="true"
            android:id="@+id/imageView" />

        <Button
            android:text="seat 10"
            android:layout_width="60dp"
            android:layout_height="70dp"
            android:id="@+id/button24"
            android:layout_alignParentBottom="true"
            android:layout_alignRight="@+id/imageView"
            android:layout_alignEnd="@+id/imageView"
            android:layout_marginRight="14dp"
            android:layout_marginEnd="14dp"
            style="@style/Widget.AppCompat.Button.Borderless.Colored" />

        <Button
            android:text="seat 11"
            android:layout_width="60dp"
            android:layout_height="70dp"
            android:id="@+id/button25"
            style="@style/Widget.AppCompat.Button.Borderless.Colored"
            android:layout_alignParentBottom="true"
            android:layout_alignLeft="@+id/imageView"
            android:layout_alignStart="@+id/imageView" />

        <Button
            android:text="seat 2"
            android:layout_width="60dp"
            android:layout_height="70dp"
            android:layout_marginTop="13dp"
            android:id="@+id/button6"
            style="@style/Widget.AppCompat.Button.Borderless.Colored"
            android:layout_below="@+id/button5"
            android:layout_alignLeft="@+id/button5"
            android:layout_alignStart="@+id/button5" />

        <TextView
            android:text="way to inside"
            android:layout_width="60dp"
            android:layout_height="70dp"
            android:textAlignment="center"
            android:rotation="90"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

    </RelativeLayout>
</RelativeLayout>
